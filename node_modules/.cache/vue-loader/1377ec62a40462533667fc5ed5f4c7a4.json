avbar.vue'\nimport Footer from '@/components/Footer.vue'\nimport axios from 'axios'\n// import Searcher from '@/components/Searcher.vue'\n\nexport default {\n\tname: 'Layout',\n\tcomponents: {\n\t\tNavbar,\n\t\tFooter\n\t\t// Searcher\n\t},\n\tdata() {\n\t\treturn {\n\t\t\tcards: null,\n\t\t\titems: [\n\t\t\t\t{\n\t\t\t\ttext: 'Inicio',\n\t\t\t\tdisabled: true,\n\t\t\t\thref: '',\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\ttext: 'Cursos',\n\t\t\t\tdisabled: false,\n\t\t\t\thref: '',\n\t\t\t\t}\n\t\t\t]   \n\t\t}\n\t},\ncomputed: {\n\tshowCards () { \n\t\tconst { cards } = this;\n\t\treturn cards;\n\t}\n},\nmounted () {\n\taxios\n\t.get('https://api.magicthegathering.io/v1/cards')\n\t.then(response => (this.cards = response.data.cards))\n}\n}\n</script>\n"]}]}